{
    "type": "package",
    "name": "andre-dietrich/elm-generic",
    "summary": "Simplify the decoding and encoding of json, xml, yaml by one simple generic API",
    "license": "BSD-3-Clause",
    "version": "1.0.0",
    "exposed-modules": [
        "Generic",
        "Generic.Decoder",
        "Generic.Json",
        "Generic.Xml",
        "Generic.Yaml"
    ],
    "elm-version": "0.19.0 <= v <= 0.20.0",
    "dependencies": {
        "Gizra/elm-all-set": "1.0.0 <= v < 2.0.0",
        "MaybeJustJames/yaml": "2.1.0 <= v < 3.0.0",
        "billstclair/elm-xml-eeue56": "2.0.0 <= v < 3.0.0",
        "edkv/elm-generic-dict": "1.0.0 <= v < 2.0.0",
        "elm/core": "1.0.0 <= v < 2.0.0",
        "elm/json": "1.0.0 <= v < 2.0.0",
        "elm/time": "1.0.0 <= v < 2.0.0",
        "rtfeldman/elm-iso8601-date-strings": "1.1.4 <= v < 2.0.0"
    },
    "test-dependencies": {
        "elm-explorations/test": "1.2.2 <= v < 2.0.0"
    }
}
